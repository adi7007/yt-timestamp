{
    "manifest_version": 3,
    "name": "You-Timestamp",
    "version": "1.0",
    "description": "Bookmark specific timestamps in YouTube videos.",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Save YouTube Timestamp"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "host_permissions": [
      "https://www.youtube.com/*"
    ]
  }
  